{
  "kind": "collectionType",
  "collectionName": "markets",
  "info": {
    "name": "Markets",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "label": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "applications": {
      "type": "dynamiczone",
      "components": [
        "features.nozzle"
      ]
    },
    "sucessfulCases": {
      "via": "market",
      "collection": "sucessful-case"
    },
    "marketPicture": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "required": true
    }
  }
}
